{
  "name": "scorebase-backend-api",
  "version": "1.0.0",
  "description": "ScoreBase Backend API - Multi-tenant, event-driven REST API for sports league management",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cdk": "cdk",
    "cdk:synth": "cdk synth",
    "cdk:deploy": "cdk deploy",
    "cdk:diff": "cdk diff",
    "cdk:destroy": "cdk destroy",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "migrate:create": "node-pg-migrate create --migration-file-language ts",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "migrate:status": "node-pg-migrate status",
    "migrate:redo": "node-pg-migrate redo"
  },
  "keywords": [
    "sports",
    "league",
    "api",
    "aws",
    "serverless",
    "multi-tenant"
  ],
  "author": "ScoreBase Team",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.17.1",
    "aws-sdk": "^2.1691.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "pg-pool": "^3.7.0",
    "source-map-support": "^0.5.21",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "aws-cdk": "^2.174.2",
    "aws-cdk-lib": "^2.174.2",
    "constructs": "^10.4.2",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "node-pg-migrate": "^8.0.4",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
